{
  "name": "@grekt-labs/cli-engine",
  "version": "5.16.2",
  "description": "Deterministic core logic for grekt CLI. Portable, testable, dependency-injected.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "imports": {
    "#/*": "./src/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run typecheck && bun build src/index.ts --outdir dist --target node && tsc --emitDeclarationOnly && tsc-alias",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grekt-labs/cli-engine.git"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "semver": "^7.7.3",
    "yaml": "^2.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^22.10.5",
    "@types/semver": "^7.7.1",
    "bun-types": "^1.3.6",
    "semantic-release": "^25.0.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "BUSL-1.1"
}
